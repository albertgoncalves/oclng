_entry_ {
    (printf "%ld\n" (fib 50 0 1))
    (f (pack 0))
    (f (pack 1 "path"))
    0
}

f x {
    unpack x {
        {
            let y (+ 1 2)
            (printf "first %d\n" y)
        }
        y {
            (printf "second %s\n" y)
        }
    }
    let y (- 1 1)
    y
}

fib n a b {
    if (= n 0) {
        a
    } else {
        (fib (- n 1) b (+ a b))
    }
}
